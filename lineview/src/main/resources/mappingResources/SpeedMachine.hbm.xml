<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.mrong.lineview.common.entity.SpeedMachine" table="speed">
        <id name="id" type="int">
            <column name="ID" />
            <generator class="native" />
        </id>
        <property name="s1" type="double">
            <column name="S1" />
        </property>
        <property name="s2" type="double">
            <column name="S2" />
        </property>
        <property name="s3" type="double">
            <column name="S3" />
        </property>
        <property name="s4" type="double">
            <column name="S4" />
        </property>
        <property name="q1" type="int">
            <column name="Q1" />
        </property>
        <property name="q2" type="int">
            <column name="Q2" />
        </property>
        <property name="sptime" type="java.util.Date">
            <column name="SPTIME" />
        </property>
    </class>
</hibernate-mapping>
