<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.mrong.lineview.common.entity.Remark" table="remark">
        <id name="id" type="java.lang.Integer" access="field">
            <column name="ID" />
            <generator class="native" />
        </id>
        <property name="teamid" type="java.lang.Integer">
            <column name="TEAM_ID" />
        </property>
        <property name="startTime" type="java.util.Date">
            <column name="START_TIME" />
        </property>
        <property name="stopTime" type="java.util.Date">
            <column name="STOP_TIME" />
        </property>
        <property name="outageClassification" type="java.lang.String">
            <column name="OC" />
        </property>
        <property name="firstClassStopClassification" type="java.lang.String">
            <column name="FCSC" />
        </property>
        <property name="secondClassStopClassification" type="java.lang.String">
            <column name="SCSC" />
        </property>
        <property name="faultDetails" type="java.lang.String">
            <column name="FAULT_DETAILS" />
        </property>
    </class>
</hibernate-mapping>
