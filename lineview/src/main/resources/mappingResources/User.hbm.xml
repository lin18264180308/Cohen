<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.mrong.lineview.common.entity.User" table="user">
        <id name="id" type="java.lang.Integer">
            <column name="ID" />
            <generator class="native" />
        </id>
        <property name="userName" type="java.lang.String">
            <column name="USER_NAME" />
        </property>
        <property name="userPassword" type="java.lang.String">
            <column name="USER_PASSWORD" />
        </property>
        <property name="permission" type="java.lang.String">
            <column name="PERMISSION" />
        </property>
        <property name="updataTime" type="java.util.Date">
            <column name="UPDATA_TIME" />
        </property>
        <property name="updatePerson" type="java.lang.String">
            <column name="UPDATE_PERSON" />
        </property>
    </class>
</hibernate-mapping>
