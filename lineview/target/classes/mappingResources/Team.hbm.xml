<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.mrong.lineview.common.entity.Team" table="team">
        <id name="id" type="java.lang.Integer">
            <column name="ID" />
            <generator class="native" />
        </id>
        <many-to-one name="user" class="com.mrong.lineview.common.entity.User" column="USER_ID" unique="true">  
        </many-to-one>
    </class>
</hibernate-mapping>
