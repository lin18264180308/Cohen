<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.mrong.lineview.operator.entity.ModeRecord" table="mode_record">
        <id name="id" type="java.lang.Integer">
            <column name="ID" />
            <generator class="native" />
        </id>
        <property name="operator" type="java.lang.String">
            <column name="OPERATOR" />
        </property>
        <property name="modeName" type="java.lang.String">
            <column name="MODE_NAME" />
        </property>
        <property name="modeClass" type="java.lang.String">
            <column name="MODE_CLASS" />
        </property>
        <property name="patternClasses" type="java.lang.String">
            <column name="PATTERN_CLASSES" />
        </property>
        <property name="startTime" type="java.util.Date">
            <column name="START_TIME" />
        </property>
        <property name="stopTime" type="java.util.Date">
            <column name="STOP_TIME" />
        </property>
        <property name="duration" type="long">
            <column name="DURATION" />
        </property>
    </class>
</hibernate-mapping>
